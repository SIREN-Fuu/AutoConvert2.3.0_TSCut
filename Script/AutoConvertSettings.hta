<html>
  <head>
    <title>AutoConvert設定</title>
    <hta:application
      id="oAutoConvert"
      applicationName="AutoConvertGUI"
      border="thin"
      innerBorder="no"
      maximizeButton="no"
      scroll="no"
      selection="no"
      version="2.2.0"
    />
    <meta http-equiv="MSThemeCompatible" content="yes">
    <style type="text/css">
      body {
        background: #f0f0f0;
      }
      input, button, select, textarea {
        width: 100%;
      }
      .main {
        width: 100%;
        font-size: 14px;
        text-align: center;
        overflow: hidden;
      }
      .container {
        width: 646px;
        margin: 0 auto;
        text-align: left;
        overflow: hidden;
      }
      .box1, .box2, .box3, .box4, .box5, .box6, .box7, .box8, .box9, .box10, .box11, .box12, .param1, .param2, .param3, .param4, .param5, .param6, .param7 {
        float: left;
        display: inline;
        margin-left: 4px;
        margin-bottom: 4px;
      }
      .box1 { width: 50px; }
      .box2 { width: 104px; }
      .box3 { width: 158px; }
      .box4 { width: 212px; }
      .box5 { width: 266px; }
      .box6 { width: 320px; }
      .box7 { width: 374px; }
      .box8 { width: 428px; }
      .box9 { width: 482px; }
      .box10 { width: 536px; }
      .box11 { width: 590px; }
      .box12 { width: 644px; }
      .param1 { width: 30px; }
      .param2 { width: 60px; }
      .param3 { width: 110px; }
      .param4 { width: 40px; }
      .param5 { width: 30px; }
      .param6 { width: 30px; }
      .param7 { width: 100px; }
      .first {
        margin-left: 0;
        clear: left;
      }
      .parent {
        padding-top: 0;
        border-top: 0;
      }
      .clearfix:after {
        visibility: hidden;
        display: block;
        font-size: 0;
        content: " ";
        clear: both;
        height: 0;
      }
      .mark {
        width:100%;
        font-size: 18px;
        margin:0 auto;
        text-align: center;
      }
      .tab {
        float: left;
        height: 25px;
        width: 150px;
        text-align: center;
        border-width: 1px;
        border-style: solid;
      }
      .tab_select {
        border-width: 1px;
        border-bottom-color: transparent;
      }
      .tab_clear {
        float: left;
        height: 25px;
        width: 4px;
        border-style: solid;
        border-width: 1px;
        border-color: transparent transparent black transparent;
      }
      * html .clearfix { zoom: 1; }
      *:first-child+html .clearfix { zoom: 1; } 
    </style>
  </head>
  <body>
    <div class="main">
      <div class="container" style="height:50px;">
        <div class="box12 first parent">
          <div class="box3 first" style="margin-left:0px;">
            <div class="tab_clear"></div>
            <div id="tab_ac" class="tab tab_select">AutoConvert</div>
            <div class="tab_clear"></div>
          </div>
          <div class="tab_clear"></div>
          <div class="box3" style="margin-left:0px;">
            <div class="tab_clear"></div>
            <div id="tab_acu" class="tab">AutoConvertUtil</div>
            <div class="tab_clear"></div>
          </div>
          <div class="tab_clear"></div>
          <div class="box3" style="margin-left:0px;">
            <div class="tab_clear"></div>
            <div id="tab_detect" class="tab">
              <select id="tab_detect_select" style="width:90%;">
                <option value="-1"> - 詳細設定 - </option>
                <option value="0"> サービス </option>
                <option value="1"> 番組 </option>
                <option value="2"> その他 </option>
              </select>
            </div>
            <div class="tab_clear"></div>
          </div>
          <div class="tab_clear"></div>
          <div class="box3" style="margin-left:0px;">
            <div class="tab_clear"></div>
            <div id="tab_preset" class="tab">
              <select id="tab_preset_select" style="width:90%;">
                <option value="-1"> - プリセット - </option>
              </select>
            </div>
            <div class="tab_clear"></div>
          </div>
        </div>
      </div>
      <div id="ac" class="container" style="height:650px;">
        <div class="box12 first">・パス</div>
        <div class="box2 first">一時パス</div>
        <div class="box3"><input id="ac_path_temppath" type="text" /></div>
